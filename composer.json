{
    "name": "nathanheffley/laravel-slack-blocks",
    "description": "Slack Blocks support for laravel notifications.",
    "keywords": ["laravel", "notifications", "slack", "blocks"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Heffley",
            "email": "nathan@nathanheffley.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "laravel/slack-notification-channel": "^2.0|^3.0"
    },
    "require-dev": {
        "illuminate/notifications": "~5.8.0|^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "NathanHeffley\\LaravelSlackBlocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NathanHeffley\\Tests\\LaravelSlackBlocks\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NathanHeffley\\LaravelSlackBlocks\\SlackChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
